<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xmlns:context="http://www.springframework.org/schema/context"
	   xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd http://www.springframework.org/schema/context https://www.springframework.org/schema/context/spring-context.xsd">

	<bean id="sysUser1" name="sysUser1" class="org.ep.springioctest.SysUser">
		<property name="username" value="老用户"/>
		<property name="sysRole" ref="sysRole1"/>
	</bean>

	<bean id="sysRole1" name="sysRole1" class="org.ep.springioctest.SysRole">
		<property name="id" value="1"/>
		<property name="name" value="sysRole1"/>
	</bean>

	<bean class="org.ep.springaoptest.MyBeanPostProcessor"></bean>
</beans>